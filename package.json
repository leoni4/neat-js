{
    "type": "module",
    "name": "neat",
    "version": "0.0.1",
    "description": "neat implementation on typescript",
    "main": "./dist/neat.min.js",
    "scripts": {
        "start": "nodemon index.cjs",
        "build": "webpack",
        "build:tsc": "tsc",
        "dev": "tsc -w",
        "type-check": "tsc --noEmit",
        "lint": "eslint --ext .js,.ts . --ignore-path .eslintignore .",
        "lint:fix": "eslint --fix --ext .js,.ts . --ignore-path .eslintignore .",
        "test": "npm run type-check && npm run lint && npm run test:unit",
        "test:unit": "jest --no-cache --coverage=false",
        "test:unit:watch": "jest --no-cache --watch --coverage=false",
        "prepare": "husky"
    },
    "keywords": [
        "neat",
        "typescript",
        "js"
    ],
    "author": "leonid lilo",
    "license": "ISC",
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "@typescript-eslint/eslint-plugin": "^8.46.3",
        "@typescript-eslint/parser": "^8.46.3",
        "canvas": "^3.2.0",
        "eslint": "^8.46.0",
        "eslint-config-prettier": "^10.1.8",
        "express": "^5.1.0",
        "husky": "^9.1.7",
        "jest": "^30.2.0",
        "konva": "^10.0.8",
        "nodemon": "^3.1.10",
        "prettier": "^3.6.2",
        "ts-jest": "^29.4.5",
        "ts-loader": "^9.5.4",
        "typescript": "^5.9.3",
        "webpack": "^5.102.1",
        "webpack-cli": "^6.0.1"
    }
}
